<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bubbles_sign_log">

    <Button
        android:id="@+id/button_sign_up"
        style="@style/Button_style"
        android:layout_width="@dimen/_264dp"
        android:layout_height="@dimen/_54dp"
        android:layout_marginStart="@dimen/_56dp"
        android:layout_marginTop="@dimen/_602dp"
        android:layout_marginEnd="@dimen/_56dp"
        android:layout_marginBottom="@dimen/_156dp"
        android:text="@string/sign_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/go_to_login_text_view"
        style="@style/Text_down_style"
        android:layout_width="@dimen/_240dp"
        android:layout_height="@dimen/_26dp"
        android:layout_marginStart="@dimen/_68dp"
        android:layout_marginTop="@dimen/_694dp"
        android:layout_marginEnd="@dimen/_68dp"
        android:layout_marginBottom="@dimen/_98dp"
        android:text="@string/sign_up_down_text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        style="@style/Image_back_style"
        android:layout_width="@dimen/_344dp"
        android:layout_height="@dimen/_412dp"
        android:layout_marginStart="@dimen/_16dp"
        android:layout_marginTop="@dimen/_88dp"
        android:layout_marginEnd="@dimen/_16dp"
        android:layout_marginBottom="@dimen/_312dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        style="@style/Text_up_style"
        android:layout_width="@dimen/_98dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginStart="@dimen/_140dp"
        android:layout_marginTop="@dimen/_120dp"
        android:layout_marginEnd="@dimen/_140dp"
        android:layout_marginBottom="@dimen/_656dp"
        android:text="@string/sign_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_340dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_420dp"
        android:hint="@string/email"
        android:importantForAutofill="no"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPassword"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_416dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_344dp"
        android:hint="@string/password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/first_name_edit_text"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_188dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_572dp"
        android:hint="@string/first_name"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/last_name_edit_text"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_264dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_496dp"
        android:hint="@string/last_name"
        android:importantForAutofill="no"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>