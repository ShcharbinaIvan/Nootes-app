<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bubbles_sign_log">

    <ImageView
        style="@style/Image_back_style"
        android:layout_width="@dimen/_344dp"
        android:layout_height="@dimen/_264dp"
        android:layout_marginStart="@dimen/_16dp"
        android:layout_marginTop="@dimen/_88dp"
        android:layout_marginEnd="@dimen/_16dp"
        android:layout_marginBottom="@dimen/_460dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_192dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_568dp"
        android:hint="@string/email"
        android:importantForAutofill="no"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPassword"
        style="@style/EditText_style"
        android:layout_width="@dimen/_296dp"
        android:layout_height="@dimen/_52dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/_268dp"
        android:layout_marginEnd="@dimen/dp_40"
        android:layout_marginBottom="@dimen/_492dp"
        android:hint="@string/password"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        style="@style/Text_up_style"
        android:layout_width="@dimen/_82dp"
        android:layout_height="@dimen/_36dp"
        android:layout_marginStart="@dimen/_148dp"
        android:layout_marginTop="@dimen/_120dp"
        android:layout_marginEnd="@dimen/_146dp"
        android:layout_marginBottom="@dimen/_656dp"
        android:text="@string/log_in"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_log_in"
        style="@style/Button_style"
        android:layout_width="@dimen/_264dp"
        android:layout_height="@dimen/_54dp"
        android:layout_marginStart="@dimen/_56dp"
        android:layout_marginTop="@dimen/_602dp"
        android:layout_marginEnd="@dimen/_56dp"
        android:layout_marginBottom="@dimen/_156dp"
        android:text="@string/log_in"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/return_to_sign_up_text_view"
        style="@style/Text_down_style"
        android:layout_width="@dimen/_148dp"
        android:layout_height="@dimen/_26dp"
        android:layout_marginStart="@dimen/_68dp"
        android:layout_marginTop="@dimen/_694dp"
        android:layout_marginEnd="@dimen/_68dp"
        android:layout_marginBottom="@dimen/_98dp"
        android:text="@string/return_to_sign_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>